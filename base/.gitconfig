[alias]
    one = "log --pretty=format:'%C(yellow)%h  %<(14)%Cred%cd %<(20)%Cblue%an%Cgreen%d %Creset%s' --date=short"

[branch "master"]
    remote = origin
    merge = refs/heads/master

[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto

[color "status"]
    added = green
    changed = yellow
    deleted = red
    untracked = red

[core]
    editor = vim
    excludesfile = ~/.gitignore

[push]
    default = current

[merge]
    conflictstyle = diff3

[user]
    name = Braden Walker
    email = bradenawalker@gmail.com

[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path = 

[diff "nodiff"]
    command = /bin/true
